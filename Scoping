'use strict';


function calAge(birthYear) {
    const age = 2022 - birthYear;
     
    function printAge() {
      const job = "Engineer"
      let output = `hi ${firstName}, you are ${age} years old and an ${job}`;
      console.log(output);

      if(birthYear >= 1990 && birthYear <2003) {
        console.log(`You are cool,  ${firstName}`);
      }
    }
    printAge();
    return age;
}

const firstName = "Ayush";

calAge(2002);
